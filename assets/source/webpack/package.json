{
    "name": "micro-world",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --open",
        "start:local": "ng serve --port 5500 --open",
        "build": "ng build",
        "build:dev": "ng build --configuration=development",
        "build:prod": "ng build --configuration=production",
        "test": "ng test",
        "rimraf": "rimraf",
        "lint": "ng lint",
        "proxy:test": "ng serve --port 1200 --open --proxy-config src/proxy.cong.json --prod",
        "ci": "npm run lint && npm run test",
        "postinstall": "npm run init-hooks",
        "init-hooks": "npm run execute:pre-commit && npm run execute:prepare-commit-msg && npm run execute:hooks-folder",
        "execute:pre-commit": "shx chmod +x .githooks/pre-commit",
        "execute:hooks-folder": "git config core.hooksPath .githooks",
        "execute:prepare-commit-msg": "shx chmod +x .githooks/prepare-commit-msg",
        "clean:dist": "rimraf -- dist"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^8.1.3",
        "@angular/cdk": "^8.2.3",
        "@angular/common": "~8.1.2",
        "@angular/compiler": "~8.1.2",
        "@angular/core": "~8.1.2",
        "@angular/forms": "~8.1.2",
        "@angular/material": "^8.2.3",
        "@angular/platform-browser": "~8.1.2",
        "@angular/platform-browser-dynamic": "~8.1.2",
        "@angular/router": "~8.1.2",
        "@ngrx/store": "^9.0.0",
        "angular-in-memory-web-api": "^0.9.0",
        "chmod": "^0.2.1",
        "eslint": "^6.8.0",
        "hammerjs": "^2.0.8",
        "npm": "6.14.4",
        "resize-observer-polyfill": "^1.5.1",
        "rimraf": "^3.0.0",
        "rxjs": "~6.4.0",
        "shx": "^0.3.2",
        "tslib": "^1.9.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.801.2",
        "@angular/cli": "~8.2.2",
        "@angular/compiler-cli": "~8.1.2",
        "@angular/language-service": "~8.1.2",
        "@types/jasmine": "~3.3.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "^5.0.0",
        "jest": "^25.2.1",
        "jest-preset-angular": "^8.1.2",
        "jest-raw-loader": "^1.0.1",
        "jest-teamcity-reporter": "^0.9.0",
        "protractor": "~5.4.0",
        "rimraf": "^3.0.0",
        "ts-jest": "^25.2.1",
        "ts-mockito": "^2.5.0",
        "ts-node": "^7.0.1",
        "tslint": "~5.15.0",
        "tslint-consistent-codestyle": "^1.16.0",
        "typescript": "~3.4.3"
    }
}
